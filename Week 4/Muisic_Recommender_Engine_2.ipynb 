    "for i in range(0,6):\n",
    "        if i ==0:\n",
    "            print('Recommendations for Music_ID: {0}\\n'.format(music_features_df.index[query_index]))\n",
    "        else:\n",
    "            print('{0} : {1} with a cosine distance of {2}'.format(i, music_features_df.index[indices.flatten()[i]], distances.flatten()[i]))  \n",
